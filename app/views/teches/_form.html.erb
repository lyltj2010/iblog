<%= form_for @tech do |f| %>
	<%= f.label :title %> <br>
	<%= f.text_field :title %> <br>

	<%= f.label :content, "New Tech Post Here" %> <br>
	<%= f.text_area :content %> <br>
	
	<%= f.label :category, 'Category' %>&nbsp
	<%= f.select :category, ['rails', 'python', 'code', 'other'] %>	
	<br>
	<br>

	<%= f.submit "Submit" %>
	
<% end %>